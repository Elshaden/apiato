{
    "name": "apiato/ship",
    "description": "Apiato Ship folder where shared functionality resides",
    "require": {
        "apiato/core": "dev-master",
        "apiato/documentation-generator-container": "^2.4.1",
        "apiato/welcome-container": "^2.0.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "barryvdh/laravel-ide-helper": "^2.9.0",
        "barryvdh/laravel-debugbar": "^3.6.2"
    },
    "scripts": {
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
